[GENERAL]
#verbose = true
polynomialOrder = 7
startFrom = "base.fld"+time=0
stopAt = endTime
endTime = 100

dt = targetCFL=0.5 + max=2e-2 + initial=5e-3
timeStepper = tombo2

checkpointControl = simulationTime 
checkpointInterval = 20

regularization = hpfrt + nModes=1 + scalingCoeff=5
constFlowRate = meanVelocity=1.0 + direction=X

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
boundaryTypeMap = zeroValue 
viscosity = 1/19000
rho = 1.0
residualTol = 1e-06

[CASEDATA]
caseType = WMLES
ReTau = 950
xLength = 6.283185307 
zLength = 3.141592653 
betaY = 2.2
number_elements_x = 16
number_elements_y = 16
number_elements_z = 16
tAvgWriteInterval=5000
