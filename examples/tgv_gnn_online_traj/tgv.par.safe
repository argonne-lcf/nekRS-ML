[GENERAL]
polynomialOrder = 7
#cubaturePolynomialOrder = 9 

#startFrom = "restart.fld"
stopAt = numSteps
numSteps = 50000

timeStepper = tombo3
dt = 1e-4

#timeStepper = tombo2
#dt = targetCFL=2.5 + max=3e-3
#subcyclingSteps = 1

checkpointControl = simulationTime 
checkpointInterval = 1000

regularization = hpfrt + nModes=1 + scalingCoeff=1

[PRESSURE]
residualTol = 1e-04

#solver = pfgmres
#smootherType = RAS+OptFourthChebyshev 
#pMGSchedule = p=7+degree=2, p=3+degree=2, p=1, p=3+degree=2, p=7+degree=2

solver = pfcg 
smootherType = Jac+Cheby

[VELOCITY]
boundaryTypeMap = none
rho = 1.0
viscosity = 1/1600
residualTol = 1e-06

[CASEDATA]
eps = 1.0
U0 = 0
V0 = 0
W0 = 0 

#eps = 0.3
#U0 = 2.0
#V0 = 2.0
#W0 = 2.0

[BOOMERAMG]
#smootherType = 18
iterations = 1
chebyshevRelaxOrder = 1
