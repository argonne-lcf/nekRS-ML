[GENERAL]
#verbose = true
polynomialOrder = 6
#startFrom = "restart.fld"
stopAt = endTime
endTime = 1000

dt = targetCFL=1 + max=2e-2 + initial=5e-3
timeStepper = tombo2

writeControl = simulationTime 
writeInterval = 20

regularization = hpfrt + nModes=1 + scalingCoeff=5
#constFlowRate = meanVelocity=1.0 + direction=X

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
boundaryTypeMap = shl
viscosity = 1/19000
density = 1.0
residualTol = 1e-06

[CASEDATA]
ReTau = 950
xLength = 6.283185307 
zLength = 3.141592653 
betaY = 2.2
number_elements_x = 16
number_elements_y = 16
number_elements_z = 16
tAvgWriteInterval=5000
