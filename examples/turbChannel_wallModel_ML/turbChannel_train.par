[GENERAL]
#verbose = true
polynomialOrder = 7
startFrom = "restart.fld"+time=0
stopAt = endTime
endTime = 400

dt = targetCFL=1 + max=2e-2 + initial=5e-3
timeStepper = tombo2

checkpointControl = simulationTime 
checkpointInterval = 20

regularization = hpfrt + nModes=1 + scalingCoeff=5
#constFlowRate = meanVelocity=1.0 + direction=X

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
boundaryTypeMap = zeroValue
viscosity = 1/19000
rho = 1.0
residualTol = 1e-06

[CASEDATA]
ReTau = 950
xLength = 6.283185307 
zLength = 3.141592653 
betaY = 2.2
tWallAvg = 10.0
number_elements_x = 36
number_elements_y = 16
number_elements_z = 18
tAvgWriteInterval=5000
